/* HW #1 Problem 1 */
#include <stdio.h>
#define N 10
double func(double x);
int main(){

        double sum=0;
        double a;

        for (a=1.0; a<N; a++){
                sum=sum+func(a);
        }
        printf("S10=%.12lf\n", sum);

return 0;

}
double func(double x){
        double y;
        y=1.0/(x*x+1);
        return y;

}
